# Local Development Environment Variables
# Copy this file to .env.development and fill in your values
# DO NOT commit .env.development to git

# NextAuth Configuration
AUTH_SECRET=your-local-auth-secret-generate-with-openssl-rand-base64-32
NEXTAUTH_URL=http://localhost:3000

# Azure AD / Microsoft Entra ID Configuration
AUTH_MICROSOFT_ENTRA_ID_TENANT_ID=your-tenant-id
AUTH_MICROSOFT_ENTRA_ID_CLIENT_ID=your-local-dev-client-id

# API Configuration
API_BASE_URL=http://localhost:8081
API_USERNAME=your-api-username
API_PASSWORD=your-api-password

# Feature Flags (optional)
ENABLE_HSTS=false

# Node Environment
NODE_ENV=development
